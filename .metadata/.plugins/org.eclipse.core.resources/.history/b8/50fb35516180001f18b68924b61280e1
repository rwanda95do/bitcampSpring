package sample04;

import java.util.Scanner;

public class SungJukInput implements SungJuk {

	@Override
	public void excute() {
		System.out.println();
		
		Scanner scan = new Scanner(System.in);
		
		System.out.print("이름 입력 : ");
		scan.next();
		System.out.print("국어점수 입력 : ");
		scan.nextInt();
		System.out.print("영어점수 입력 : ");
		scan.nextInt();
		System.out.print("수학점수 입력 : ");
		scan.nextInt();
	}

}
