package sample04;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

@Component
public class SungJukDTO2 {
	private String name;
	private int kor;
	private int eng;
	private int math;
	private int tot;
	private double avg;
	
	
// setter
	@Autowired
	public void setName(String name) {
		this.name = name;
	}
	@Autowired
	public void setKor(int kor) {
		this.kor = kor;
	}
	@Autowired
	public void setEng(int eng) {
		this.eng = eng;
	}
	@Autowired
	public void setMath(int math) {
		this.math = math;
	}
	@Autowired
	public void setTot(int tot) {
		this.tot = tot;
	}
	@Autowired
	public void setAvg(double avg) {
		this.avg = avg;
	}


// getter
	public String getName() {
		return name;
	}
	public int getKor() {
		return kor;
	}
	public int getEng() {
		return eng;
	}
	public int getMath() {
		return math;
	}
	public int getTot() {
		return tot;
	}
	public double getAvg() {
		return avg;
	}
	
	
	@Override
	public String toString() {
		return name + "   " + kor + "   " + eng + "   " + math + "   " + tot + "   " + avg ;
	}
}
